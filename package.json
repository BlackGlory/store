{
  "private": true,
  "name": "store",
  "version": "0.3.13",
  "repository": "git@github.com:BlackGlory/store.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js --no-cache",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "dev": "nodemon",
    "dev:http2": "cross-env STORE_HTTP2=true nodemon",
    "clean": "rimraf lib dist",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths --project tsconfig.build.json --src ./src --out ./lib",
    "bundle": "webpack --stats-error-details",
    "start": "node dist/index.cjs",
    "start:http2": "cross-env STORE_HTTP2=true node dist/index.cjs",
    "smoke": "cross-env NODE_ENV=test node dist/index.cjs",
    "docker:build": "docker build --tag store .",
    "docker:test": "docker-compose --file docker-compose.test.yml run --rm test",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s clean lint build test bundle smoke",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/http-status": "^1.0.2",
    "@blackglory/jest-matchers": "^0.3.1",
    "@blackglory/log": "^1.2.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/accepts": "^1.3.5",
    "@types/app-root-path": "^1.2.4",
    "@types/better-sqlite3": "^7.6.0",
    "@types/jest": "^27.4.1",
    "@types/jest-json-schema": "^6.1.1",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "extra-fetch": "^1.8.5",
    "extra-request": "^4.0.1",
    "extra-response": "^0.3.11",
    "husky": "4",
    "is-stream": "^3.0.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-json-schema": "^6.1.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^7.6.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yarn-deduplicate": "^5.0.0"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.15",
    "@blackglory/errors": "^2.2.2",
    "@blackglory/go": "^1.1.2",
    "@blackglory/types": "^1.2.1",
    "ajv": "^8.11.0",
    "app-root-path": "^3.0.0",
    "better-sqlite3": "^7.6.2",
    "dotenv": "^16.0.1",
    "extra-filesystem": "^0.4.5",
    "extra-generator": "^0.2.17",
    "extra-lazy": "^1.3.1",
    "extra-promise": "^2.4.1",
    "extra-semver": "^0.1.0",
    "extra-tags": "^0.2.5",
    "fastify": "^3.28.0",
    "fastify-accepts": "^2.2.0",
    "fastify-bearer-auth": "^6.2.0",
    "fastify-cors": "^6.0.3",
    "fastify-metrics": "^8.0.0",
    "fastify-plugin": "^3.0.1",
    "iterable-operator": "^1.2.0",
    "justypes": "^2.0.1",
    "migration-files": "^0.4.0",
    "nanoid": "^3.3.2",
    "prom-client": "^14.0.1",
    "return-style": "^1.0.0",
    "value-getter": "^0.2.4",
    "you-died": "^0.4.1"
  }
}
